#include <stdio.h>

typedef struct {
	int arr[max_size];
	int top;
} Stack;

void initialize(Stack *stack){
	stack->top = -1;
}

void push(Stack *stack, int value){
  if(isFull(stack) == true){
    printf("Stack overflow");
    return;
  }
  else{
    stac->top++;
    stack->arr[top] = value;
  }
}

bool isFull(Stack *stack){
  if(stack->top == max_size-1){
    return true;
  }
  else{
    return false;
  }
}
int pop(Stack *stack){
  if(empty(stack)==true){
    printf("Stack underflow");
    return;
  }
  else{
    int popped = stack->arr[top];
    stack->top--;
    return popped;
  }
}

int topStack(Stack *stack){
  if(empty(stack) == true){
    printf("Stack is empty");
  }
  else{
    int topElement=stack->arr[top];
  }
}

bool empty(Stack *stack){
	//returns true or false
  if(stack->top == -1){
    return true;
  }
  else{
    return false;
  }
}

int main(){
  Stack stack;
  initialize(&stack);

  push(stack,5);

  return 0;
}
